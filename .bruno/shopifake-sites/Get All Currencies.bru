  type: http
  seq: 10
}

get {
  url: {{baseUrl}}/api/sites/currencies
  body: none
  auth: none
}

headers {
  Content-Type: application/json
}

vars:pre-request {
  baseUrl: http://localhost:8080
}

docs {
  # Get All Currencies
  
  Retrieves all supported currency codes.
  
  ## Response Format
  Returns:
  - `currencies`: Array of supported currency codes (USD, EUR, GBP, JPY, CAD, AUD, CHF, CNY)
  - `count`: Number of supported currencies
}

