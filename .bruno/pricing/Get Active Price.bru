meta {
  name: Get Active Price
  type: http
  seq: 33
}

get {
  url: {{baseUrl}}/api/prices/product/{{productId}}/active
  auth: none
}

headers {
  Accept: application/json
}

vars:pre-request {
  baseUrl: http://localhost:8083
  productId: {{env.productId}}
}

docs {
  # Get Active Price

  Fetches the price currently in effect for the product. Returns HTTP 404 if no active price exists.
}

